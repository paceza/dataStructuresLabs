class Number {
public:
  
  Number (int n) {intNumber = n;}
  Number (std::string s) [strNumber = s;}

  bool isALegalRomanNumber(); //call to determining method

  int intValue(); //converts RN to int

  std::string romanValue(); //converts  int to RN

private:
  std::string strNumber;
  int intNumber;
};
  
